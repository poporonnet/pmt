{
    "rand": {
        "snippet": "rand(${1:1}..${2:10})",
        "description": "(1) から (10) までの乱数"
    },
    "and": {
        "snippet": "${1:false} && ${2:false}",
        "description": "< > かつ < >"
    },
    "or": {
        "snippet": "${1:false} || ${2:false}",
        "description": "< > または < >"
    },
    "not": {
        "snippet": "!${1:false}",
        "description": "< > ではない"
    },
    "string+": {
        "snippet": "\"${1:りんご}\" + \"${2:バナナ}\"",
        "description": "(りんご) と (バナナ)"
    },
    "string[]": {
        "snippet": "\"${1:りんご}\"[${2:1}]",
        "description": "(りんご) の (1) 番目の文字"
    },
    "string.length": {
        "snippet": "\"${1:りんご}\".length",
        "description": "(りんご) の長さ"
    },
    "string.include?": {
        "snippet": "\"${1:りんご}\".include?(\"${2:り}\")",
        "description": "(りんご) に (り) が含まれる"
    },
    "round": {
